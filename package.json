{
  "name": "ecoach",
  "version": "1.0.0",
  "main": "index.js",
  "author": "James Brooks <james@brookode.uk>",
  "license": "MIT",
  "scripts": {
    "build:server": "tsc --project tsconfig.server.json",
    "build": "rm -rf build && rm -rf .next && next build && npm run build:server",
    "start": "NODE_ENV=production node build/index.js",
    "dev": "concurrently \"next dev\" \"nodemon\"",
    "db:new": "knex --knexfile src/knexfile.ts -x ts migrate:make",
    "db:down": "knex --knexfile src/knexfile.ts migrate:down",
    "heroku:push": "heroku container:push web -a e-coach-staging",
    "heroku:deploy": "heroku container:release web -a e-coach-staging"
  },
  "dependencies": {
    "auth0": "^2.24.0",
    "concurrently": "^5.1.0",
    "dotenv": "^8.2.0",
    "envalid": "^6.0.1",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "graphile-build-pg": "^4.5.3",
    "graphql-middleware": "^4.0.2",
    "graphql-shield": "^7.2.2",
    "jwks-rsa": "^1.7.0",
    "knex": "^0.20.13",
    "next": "^9.3.4",
    "pg": "^8.0.0",
    "postgraphile": "^4.6.0",
    "postgraphile-plugin-connection-filter": "^1.1.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "typescript": "^3.8.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/node": "^13.11.0",
    "@types/react": "^16.9.32",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.2",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.2",
    "ts-node": "^8.8.1"
  }
}
